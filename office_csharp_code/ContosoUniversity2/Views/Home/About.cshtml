@model IEnumerable<ContosoUniversity2.Models.SchoolViewModels.EnrollmentDateGroup>

@{
    ViewData["Title"] = "Student by Statistics";
}

<h2>Student Body Statistics</h2>

<table>
    <tr>
        <th>Enrollment Date</th>
        <th>Student</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EnrollmentData)
            </td>
            <td>
                @item.StudentCount
            </td>
        </tr>
    }
</table>